<SetViewMode>504</SetViewMode>

<item>
	<title>[COLOR red]  DEPORTES (Arenavisión)[/COLOR] </title>
	<link>$doregex[makelist]</link>
		<regex>
			<name>makelist</name>
			
			<listrepeat><![CDATA[
				<item>
					<title> [COLOR red] [makelist.param2][/COLOR]</title>
					<link>plugin://program.plexus/?url=$doregex[hash]&amp;mode=1&amp;name=Adryanlist + [makelist.param2]</link>
							<regex>
								<name>hash</name> 
								
								<expres>
									this.loadPlayer\("(.*?)"
								</expres>
								
								<page>acelisting.in/[makelist.param1]</page>
								<referer>acelisting.in</referer>
								<setcookie>beget=begetok</setcookie>
							</regex>
					<thumbnail>https://github.com/Frange/test/raw/master/test-tube.png</thumbnail>
				</item>	
			]]></listrepeat>
			
			<expres>
				<![CDATA[
					href="acestream:\/\/(.*)"
				]]>
			</expres>
			
			<page>acelisting.in</page>
			<Agent>Mozilla</Agent>
			<referer>acelisting.in</referer>
			<setcookie>beget=begetok</setcookie>
		</regex>
	
	<thumbnail>https://github.com/Frange/test/raw/master/acestreams.jpg</thumbnail>
	<info>[COLOR white]Canales de Arenavisión para ver todo tipo de eventos deportivos[/COLOR]</info>
</item>
